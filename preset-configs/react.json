{
  "transforms": {
    "js": {
      "transformer": "jsx",
      "config": {
        "babelOptions": {
          "presets": ["es2015", "es2016", "es2017", "stage-2"],
          "plugins": ["babel-plugin-transform-decorators-legacy"]
        }
      }
    },
    "jsx": {
      "transformer": "jsx",
      "config": {
        "babelOptions": {
          "presets": ["es2015", "es2016", "es2017", "stage-2"],
          "plugins": ["babel-plugin-transform-decorators-legacy"]
        }
      }
    },
    "svg@jsx": "svg-sprite"
  }
}